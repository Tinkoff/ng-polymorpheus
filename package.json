{
    "name": "ng-polymorpheus",
    "private": true,
    "description": "This is a small library for customizing components appearance",
    "keywords": [
        "angular",
        "ng",
        "template",
        "polymorph",
        "component",
        "function",
        "context",
        "ng-template",
        "TemplateRef",
        "ngTemplateOutlet",
        "ngComponentOutlet"
    ],
    "homepage": "https://github.com/taiga-family/ng-polymorpheus#README",
    "bugs": "https://github.com/taiga-family/ng-polymorpheus/issues",
    "repository": "https://github.com/taiga-family/ng-polymorpheus",
    "license": "Apache-2.0",
    "author": {
        "name": "Alex Inkin",
        "email": "alexander@inkin.ru"
    },
    "contributors": [
        "Roman Sedov <79601794011@ya.ru> (http://marsibarsi.me/)"
    ],
    "scripts": {
        "start": "nx serve demo",
        "dev:ssr": "nx run demo:serve-ssr",
        "build:ssr": "nx build --prod && nx run demo:server:production",
        "prerender": "nx run demo:prerender",
        "build": "nx build demo",
        "prettier": "prettier !package-lock.json .  --ignore-path .gitignore",
        "test": "nx test ng-polymorpheus",
        "lint": "eslint .",
        "typecheck": "tsc --noEmit --skipLibCheck"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged && npm run typecheck"
        }
    },
    "lint-staged": {
        "*.{js,ts,html,md,less,json}": [
            "npm run lint -- --fix",
            "prettier --write",
            "git add"
        ]
    },
    "browserslist": [
        "extends @taiga-ui/browserslist-config"
    ],
    "prettier": "@taiga-ui/prettier-config",
    "dependencies": {
        "@angular/animations": "15.2.10",
        "@angular/common": "15.2.10",
        "@angular/compiler": "15.2.10",
        "@angular/core": "15.2.10",
        "@angular/forms": "15.2.10",
        "@angular/platform-browser": "15.2.10",
        "@angular/platform-browser-dynamic": "15.2.10",
        "@angular/platform-server": "15.2.10",
        "@angular/router": "15.2.10",
        "@ng-web-apis/common": "3.0.6",
        "@ng-web-apis/universal": "3.0.7",
        "@nguniversal/express-engine": "15.2.1",
        "@tinkoff/ng-dompurify": "4.0.0",
        "dompurify": "3.0.8",
        "express": "4.18.2",
        "ngx-highlightjs": "6.1.3",
        "rxjs": "7.8.1",
        "tslib": "2.6.2",
        "zone.js": "0.12.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "15.2.10",
        "@angular-devkit/core": "15.2.10",
        "@angular-eslint/builder": "17.2.1",
        "@angular-eslint/eslint-plugin": "17.2.1",
        "@angular-eslint/eslint-plugin-template": "17.2.1",
        "@angular-eslint/schematics": "17.2.1",
        "@angular-eslint/template-parser": "17.2.1",
        "@angular/cli": "15.2.10",
        "@angular/compiler-cli": "15.2.10",
        "@angular/language-service": "15.2.10",
        "@commitlint/cli": "18.6.0",
        "@commitlint/config-conventional": "18.6.0",
        "@nguniversal/builders": "15.2.1",
        "@nx/angular": "17.2.8",
        "@nx/jest": "17.2.8",
        "@nx/node": "17.2.8",
        "@nx/workspace": "17.2.8",
        "@swc/helpers": "0.5.3",
        "@taiga-ui/browserslist-config": "0.4.0",
        "@taiga-ui/eslint-plugin-experience": "0.47.3",
        "@taiga-ui/prettier-config": "0.7.0",
        "@tinkoff/eslint-config": "2.0.0",
        "@tinkoff/linters": "1.52.1",
        "@types/dompurify": "3.0.5",
        "@types/estree": "1.0.5",
        "@types/express": "4.17.21",
        "@types/jest": "29.5.11",
        "@types/node": "20.11.10",
        "@typescript-eslint/eslint-plugin": "6.13.2",
        "@typescript-eslint/parser": "6.13.2",
        "eslint": "8.56.0",
        "highlight.js": "11.9.0",
        "husky": "9.0.7",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "jest-preset-angular": "12.2.6",
        "lerna": "8.0.2",
        "lint-staged": "15.2.0",
        "ng-packagr": "15.2.2",
        "nx": "17.2.8",
        "prettier": "3.2.4",
        "stylelint": "16.2.0",
        "ts-jest": "29.1.2",
        "ts-node": "10.9.2",
        "tsickle": "0.46.3",
        "tsutils": "3.21.0",
        "typescript": "4.8.2",
        "wait-on": "7.2.0",
        "webpack": "5.90.0"
    },
    "engines": {
        "node": ">= 10",
        "npm": ">= 3"
    },
    "schematics": "./schematics/collection.json",
    "standard-version": {
        "scripts": {
            "postbump": "node scripts/syncVersions.js && git add **/package.json"
        }
    }
}
